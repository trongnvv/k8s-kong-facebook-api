{
  "name": "template",
  "version": "1.0.0",
  "main": "./bin/run",
  "author": "ACE",
  "license": "MIT",
  "scripts": {
    "start": "node ./bin/run",
    "dev": "cross-env NODE_ENV=development nodemon -e yaml,js,json",
    "start:dev": "cross-env NODE_ENV=development nodemon ",
    "start:stg": "cross-env NODE_ENV=staging node ./bin/run",
    "start:prod": "cross-env NODE_ENV=production node ./bin/run",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000 --detectOpenHandles --forceExit",
    "lint": "eslint __tests__ src --cache",
    "lint:fix": "yarn lint --fix",
    "lint:staged": "eslint __tests__ src --fix",
    "format": "prettier --write \"src/**/*.js\" \"__tests__/**/*.js\""
  },
  "dependencies": {
    "@trongnv/backend-helper": "0.0.3",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "debug": "^4.2.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "http-status-codes": "^1.4.0",
    "joi": "^17.2.1",
    "js-base64": "^3.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.32",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "eslint": "^7.26.0",
    "husky": "^4.3.0",
    "jest": "^26.6.0",
    "lint-staged": "^10.4.2",
    "log4js": "^6.1.1",
    "nodemon": "^1.19.1",
    "prettier": "^2.1.2",
    "supertest": "^5.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "resolutions": {
    "chokidar": "^3.4.0"
  },
  "husky": {},
  "lint-staged": {
    "*.js": [
      "yarn lint:staged",
      "yarn format",
      "git add ."
    ]
  }
}
